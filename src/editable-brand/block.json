{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "armoireplus-blocks/editable-brand",
	"title": "Armoireplus Editable Brand Block",
	"textdomain": "armoireplus-editable-brand-block",
	"category": "blocs-perso",
	"keywords": [ "brand","armoireplus"],
	"attributes": {
		"brand": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		},
		"align": true
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php",
	"style": "file:./style-index.css"
}
